    "version": "0.2.0",
    "configurations": [{
            "name": "Java",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            //"preLaunchTask": "build",
            "jdkPath": "C:/Program Files/Java/jdk1.8.0_162/bin",
            "cwd": "${workspaceRoot}",
            "startupClass": "client", // The class you want to run
            "startupClassPathPattern": "src/server/",
            "sourcePath": ["${workspaceRoot}"], // Indicates where your source (.java) files are
            "classpath": ["${workspaceRoot}"],
            "args": [
                "Gui", "30", "0", "lowmem", "members", "32"
            ]
        },
        {
            "name": "Java Console App",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "C:/Program Files/Java/jdk1.8.0_162/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ],
            "externalConsole": true
        }
    ]
}